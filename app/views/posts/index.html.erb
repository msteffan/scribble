
<div class="post">
    <% @posts.each do |post| %>
        <h3><%= link_to post.title, post_path(post) %></h3>
        <h4>Scribbled by <%= post.author + " on " + post.post_date %></h4>
        <%= image_tag src="#{post.photo_url}" %>
        <p>
            <%= post.text %>
        </p>
    <% end %>
</div>
