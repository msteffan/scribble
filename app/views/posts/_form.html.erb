
<%= form_for (@post) do |f| %>
<p>
    <%= f.label :title %>
    <%= f.text_field :title %>
</p>
<p>
    <%= f.label :author %>
    <%= f.text_field :author %>
</p>
<p>
    <%= f.label :text %>
    <%= f.text_area :text %>
</p>
    <%= f.label :post_date %>
    <%= f.text_field :post_date %>
<p>
    <%= f.label :photo_url %>
    <%= f.text_field :photo_url %>
</p>
<p>
    <%= f.submit "Scribble" %>
</p>
<% end %>
